									"    pm.expect(response[0].name).to.eql(\"tomato\");\r",
									"    pm.expect(response[1].name).to.eql(\"lettuce\");\r",
									"    pm.expect(response[2].name).to.eql(\"apple\");\r",
									"});\r",
									"\r",
									"pm.test(\"Test value has incorrect name\", () => {\r",
									"    pm.expect(response[0].name).to.eql(\"cheese\");\r",
									"    pm.expect(response[1].name).to.eql(\"juice\");\r",
									"    pm.expect(response[2].name).to.eql(\"orange\");\r",
									"});\r",
									"\r",
									"pm.test(\"All id are correct\", () => {\r",
									"    let len = response.length\r",
									"    for (let i = 1; i < len; i++) {\r",
									"        pm.expect(response[i].id - response[i-1].id).to.be.eql(1); \r",
									"    }\r",
									"});\r",
									"\r",
									"pm.test(\"All id are unic\", () => {\r",
									"  let count = 0\r",
									"  for (let i = 0; i < response.length; i++){\r",
									"      for (let j = 0; j < response.length; j++){\r",
									"        if(response[i].id === response[j].id){\r",
									"              count += 1\r",
									"        }\r",
									"      } \r",
									"  }\r",
									"  pm.expect(count).to.be.eql(response.length)  \r",
									"})\r",
									"\r",
									"pm.test(\"Id are not unic\", () => {\r",
									"  let count = 0\r",
									"  for (let i = 0; i < response.length; i++){\r",
									"      for (let j = 0; j < response.length; j++){\r",
									"        if(response[i].id === response[j].id){\r",
									"              count += 1\r",
									"        }\r",
									"      } \r",
									"  }\r",
									"  pm.expect(count).not.to.be.eql(response.length)  \r",
									"})"
								],
